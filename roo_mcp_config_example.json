{
  "mcpServers": {
    "github-trending-mcp": {
      "command": "python",
      "args": [
        "-m",
        "github_trending_mcp"
      ],
      "alwaysAllow": [
        "get_github_trending",
        "get_proxy_status"
      ],
      "disabled": false,
      "env": {
        "PROXY_HOST": "127.0.0.1",
        "PROXY_PORT": "10808",
        "PROXY_PROTOCOL": "socks5"
      }
    }
  }
}
